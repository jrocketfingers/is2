    <connection>Operational DB</connection>
    <sql>SELECT &#xa;	COUNT&#x28;&#x60;t1&#x60;.&#x60;id&#x60;&#x29; AS number_of,&#xa;	&#x28;SELECT analytics.article.id&#xa;	 FROM analytics.article&#xa;	 WHERE&#xa;		&#x60;t3&#x60;.&#x60;name&#x60; &#x3d; analytics.article.name AND&#xa;		&#x60;t9&#x60;.&#x60;name&#x60; &#x3d; analytics.article.type AND&#xa;		&#x60;t6&#x60;.&#x60;name&#x60; &#x3d; analytics.article.color AND&#xa;		&#x60;t7&#x60;.&#x60;name&#x60; &#x3d; analytics.article.size&#x29; AS &#x27;article_id&#x27;,&#xa;	&#x28;SELECT analytics.price.id&#xa;	 FROM analytics.price&#xa;	 WHERE&#xa;		&#x60;t1&#x60;.&#x60;price&#x60; &#x3d; analytics.price.value&#x29; AS &#x27;price_id&#x27;,&#xa;	&#x60;t4&#x60;.&#x60;PIB&#x60; AS &#x27;seller_id&#x27;,&#xa;	&#x28;SELECT analytics.offertime.id&#xa;	 FROM analytics.offertime&#xa;	 WHERE&#xa;		analytics.offertime.date &#x3d; CURDATE&#x28;&#x29;&#x29; AS &#x27;time_id&#x27;&#xa;FROM &#x60;offer&#x60; AS t1&#xa;	INNER JOIN &#xa;		&#x28;SELECT&#xa;			&#x60;t12&#x60;.&#x60;PIB&#x60; AS seller_id,&#xa;			&#x60;t11&#x60;.&#x60;id&#x60; AS article_id,&#xa;			MAX&#x28;&#x60;t10&#x60;.&#x60;created_at&#x60;&#x29; AS created_at&#xa;		 FROM &#x60;offer&#x60; AS t10&#xa;			INNER JOIN &#x60;article&#x60; AS t11 ON &#x28;&#x60;t10&#x60;.&#x60;article_id&#x60; &#x3d; &#x60;t11&#x60;.&#x60;id&#x60;&#x29;&#xa;			INNER JOIN &#x60;seller&#x60; AS t12 ON &#x28;&#x60;t12&#x60;.&#x60;PIB&#x60; &#x3d; &#x60;t10&#x60;.&#x60;seller_id&#x60;&#x29;&#xa;		 GROUP BY &#x60;t12&#x60;.&#x60;PIB&#x60;, &#x60;t11&#x60;.&#x60;id&#x60;&#x29; AS last_offer_date&#xa;	ON &#x28;&#x28;&#x28;&#x60;t1&#x60;.&#x60;article_id&#x60; &#x3d; &#x60;last_offer_date&#x60;.&#x60;article_id&#x60;&#x29; AND &#x28;&#x60;t1&#x60;.&#x60;seller_id&#x60; &#x3d; &#x60;last_offer_date&#x60;.&#x60;seller_id&#x60;&#x29;&#x29; AND &#x28;&#x60;t1&#x60;.&#x60;created_at&#x60; &#x3d; &#x60;last_offer_date&#x60;.&#x60;created_at&#x60;&#x29;&#x29;&#xa;	INNER JOIN &#x60;article&#x60; AS t3 ON &#x28;&#x60;t1&#x60;.&#x60;article_id&#x60; &#x3d; &#x60;t3&#x60;.&#x60;id&#x60;&#x29;&#xa;	INNER JOIN &#x60;type&#x60; AS t9 ON &#x28;&#x60;t3&#x60;.&#x60;type_id&#x60; &#x3d; &#x60;t9&#x60;.&#x60;id&#x60;&#x29;&#xa;	INNER JOIN &#x60;color&#x60; AS t6 ON &#x28;&#x60;t3&#x60;.&#x60;color_id&#x60; &#x3d; &#x60;t6&#x60;.&#x60;id&#x60;&#x29;&#xa;	INNER JOIN &#x60;size&#x60; AS t7 ON &#x28;&#x60;t3&#x60;.&#x60;size_id&#x60; &#x3d; &#x60;t7&#x60;.&#x60;id&#x60;&#x29;&#xa;	INNER JOIN &#x60;seller&#x60; AS t4 ON &#x28;&#x60;t1&#x60;.&#x60;seller_id&#x60; &#x3d; &#x60;t4&#x60;.&#x60;PIB&#x60;&#x29;&#xa;	INNER JOIN &#x60;district&#x60; AS t5 ON &#x28;&#x60;t4&#x60;.&#x60;district_id&#x60; &#x3d; &#x60;t5&#x60;.&#x60;id&#x60;&#x29;&#xa;	INNER JOIN &#x60;city&#x60; AS t8 ON &#x28;&#x60;t5&#x60;.&#x60;city_id&#x60; &#x3d; &#x60;t8&#x60;.&#x60;id&#x60;&#x29;&#xa;GROUP BY&#xa;	&#x60;t1&#x60;.&#x60;price&#x60;, &#xa;	&#x60;t3&#x60;.&#x60;id&#x60;,&#xa;	&#x60;t4&#x60;.&#x60;PIB&#x60;</sql>
